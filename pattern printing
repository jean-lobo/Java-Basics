class Main {
  public static void main(String[] args) {
int n=8;
for(int i=1;i<=n;i++){
   System.out.print("* ");
}
 System.out.println();    
for(int i=2;i<=n-1;i++){
  for(int j=1;j<=n;j++){
    if(j==1){
        System.out.print("* ");
    }
}
  System.out.println(); 
}


for(int i=1;i<=n;i++){
   System.out.print("* ");
  
} 
  }
}

